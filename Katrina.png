$(document).ready(function() {
    $('.subscribe').click(function() {
      var btn = $(this);
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        var s_email = $(this).closest("div.col").find("input[name='s_email']").val();
         var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        if (s_email=='') {
        $(this).closest("div.col").find(".success").text('Please fill out this field').fadeIn(500);
        $(this).closest("div.col").find(".success").fadeOut(3000);
        } else if (!filter.test(s_email)){
          $(this).closest("div.col").find(".success").text('Please Enter valid email').fadeIn(500);
          return false;
        }else{
        $.ajax({
             type:"POST",
              data: {s_email : s_email},
             url:"https://www.exchange4media.com/subscribenewsletter-info",
             success : function(results) {
                btn.closest("div.col").find('.mail_success').text(results).fadeIn(500);
                btn.closest("div.col").find('.mail_success').fadeOut(5000);
                $('.contact_form').trigger("reset");
             }
        });
      }
    });
});
</script>

